class Solution {
public:
    int findDuplicate(vector<int>& nums) {
        int i=0;
        int n=nums.size();
        while(i<n){
            int curridx=nums[i]-1;
            if(i==curridx || nums[i]==nums[curridx]) i++;
            else swap(nums[i],nums[curridx]);
        }
        for(int i=0;i<n;i++) if(nums[i]!=i+1) return nums[i];
        return -1;
    }
};
